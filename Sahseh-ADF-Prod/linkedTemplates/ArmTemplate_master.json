{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Sahseh-ADF-Prod"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureSqlDados_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDados'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sahsehbidatalake.dfs.core.windows.net/"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Sahseh-Vault.vault.azure.net/"
		},
		"AzureSqlDados_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserveritcantestandprod.database.windows.net"
		},
		"AzureSqlDados_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_sql_prod_dados"
		},
		"AzureSqlDados_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Sahseh-ADF"
		},
		"AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserversahsehbi.database.windows.net"
		},
		"AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_sql_prod_sahseh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Sahseh-ADF-Prod_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureSqlDados_password": {
						"value": "[parameters('AzureSqlDados_password')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureSqlDados_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDados_properties_typeProperties_server')]"
					},
					"AzureSqlDados_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDados_properties_typeProperties_database')]"
					},
					"AzureSqlDados_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDados_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}